/* pages/page_2/page_2.wxss */
.container{
  display: flex;
  flex-direction:column;
  font-family: Arial, sans-serif;
}

.page-body{
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}

/* '设备发现数量'提示设置内容 */
.devices-status{
  position: fixed;
  top: 2.5%;
  align-items: center;
  font-size: 26rpx;
}

/* 温度显示模块设置内容 */
.tem-container{
  position: fixed;
  top: 8.5%;
  left: 23%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}
.icontemperature{
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.icon-temperature{
  height: 48rpx;
  width: 48rpx;
}

/* 湿度显示模块设置内容 */
.hum-container{
  position: fixed;
  top: 15.5%;
  left: 23%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}
.iconhumidity{
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.icon-humidity{
  height: 48rpx;
  width: 48rpx;
}

/* 电压显示模块设置内容 */
.volt-container{
  position: fixed;
  top: 22.5%;
  left: 23%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}
.iconvoltage{
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.icon-voltage{
  height: 48rpx;
  width: 48rpx;
}

.text-num{
  font-size: 44rpx;
  color: black;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
  
}

.setparam-data{
  position: fixed;
  top: 30%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.text-setparam-data{
  font-size: 30rpx;
  color: black;
  display: flex;
  align-items: center;
  justify-content: center; 
}

/* 选择器设置内容 */
.selector_precision{
  position: fixed;
  height: 15%;
  top: 34%;
  left: 15%;
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}
.selector_frequency{
  position: fixed;
  height: 15%;
  top: 34%;
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}
.selector_duration{
  position: fixed;
  height: 15%;
  top: 34%;
  right: 15%;
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}
.selector_title{
  font-size: 30rpx;
}
.picker{
  margin-top: 20rpx;
  height: 85rpx;
  width: 120rpx;
  font-size: 30rpx;
  color: black;
  background-color: whitesmoke;
  border-color: gray;
  border-style: solid;
  border-width: 1rpx;
  border-radius: 20rpx;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}

/* '确认参数开始测试'按钮的设置内容 */
.setdata{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.btn-setdata{
  height: 7.5%;
  width: 75%;
  position: fixed;
  bottom: 42%;
  right: 12.5%;
  background-color: rgb(245, 168, 75);
  border-radius: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.btn-setdata::after{/*用border::after来删除button的默认边宽*/
  border-radius: 100rpx;
  border: 0;
}
.btn-setdata-hover{
  background-color: burlywood;
}
.btn-setdata-wait{
  height: 7.5%;
  width: 75%;
  position: fixed;
  bottom: 42%;
  right: 12.5%;
  background-color: burlywood;
  border-radius: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.btn-setdata-wait::after{/*用border::after来删除button的默认边宽*/
  border-radius: 100rpx;
  border: 0;
}
.text-setdata{
  font-size: 36rpx;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center; 
}

.setdata-time{
  position: fixed;
  bottom: 39.5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.text-data-time{
  font-size: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center; 
}

.realtime-data{
  position: fixed;
  bottom: 35.5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.text-realtime-data{
  font-size: 30rpx;
  color: black;
  display: flex;
  align-items: center;
  justify-content: center; 
}

