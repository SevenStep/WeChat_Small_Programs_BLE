/* pages/test/test.wxss */
.container{
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}

.page-body{
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center; 
}

.devices-status{
  position: fixed;
  top: 2.5%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
  color: antiquewhite;
}

.tem-container{
  position: fixed;
  top: 8%;
  left: 12.5%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}
.icontemperature{
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.icon-temperature{
  height: 24px;
  width: 24px;
}

.hum-container{
  position: fixed;
  top: 15%;
  left: 12.5%;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
}
.iconhumidity{
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.icon-humidity{
  height: 24px;
  width: 24px;
}

.text-num{
  font-size: 22px;
  color: black;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: center; 
  
}

.btn-close{
  position: fixed;
  top: 10%;
  right: 12.5%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.devices-finder{
  position: fixed;
  top: 22%;
  left: 10%;
}

.devices-list{
  position: fixed;
  top: 27%;
  left:5%;
  width: 90%;
  height: 50%;
  background-color: black;
}

.btn-satrt{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.button1{
  height: 50px;
  width: 75%;
  position: absolute;
  bottom: 12.5%;
  right: 12.5%;
  background-color: burlywood;
  color: white;
  border-radius: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.button1::after {
  border-radius: 100rpx;
  border: 0;
}
/*用border::after来删除button的默认边宽*/
.button1-hover{
  background-color: rgb(226, 203, 174);
}
.icon-bluetooth{
  background-image: url(data:image/svg+xml;base64,);
  height: 20px;
  width: 20px;
  background-size: cover;
  display: flex;
  align-items: center;
  justify-content: center; 
}

.developer{
  position: fixed;
  bottom: 7.5%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center; 
}
.text-name{
  font-size: smaller;
  color: gray;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center; 
  
}
